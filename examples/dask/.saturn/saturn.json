{
  "name": "dask",
  "image": "saturncloud/saturn:2021.09.20",
  "description": "Use distributed computing with Dask",
  "environment_variables": {
    "SATURN__JUPYTER_SETUP_DASK_WORKSPACE": "true"
  },
  "working_dir": "/home/jovyan/git-repos/examples/examples/dask",
  "git_repos": [
    {
      "remote_url": "https://github.com/saturncloud/examples",
      "ref": "release-2021.09.20",
      "location": "examples"
    }
  ],
  "jupyter_server": {
    "start_ssh": false,
    "disk_space": "10Gi",
    "instance_type": "large",
    "auto_shutoff": "1 hour"
  },
  "dask_cluster": {
    "n_workers": 3,
    "worker": {
      "instance_type": "large",
      "nprocs": 1,
      "nthreads": 1,
      "is_spot": false
    },
    "scheduler": {
      "instance_type": "large"
    }
  }
}
