{
  "name": "example-shutoff-jkernels",
  "image_uri": "saturncloud/saturn:2021.09.20",
  "description": "POC of using Jupyter kernel information for shutting off a resource",
  "working_directory": "/home/jovyan/git-repos/examples/examples/shutoff-jkernels",
  "environment_variables": {
    "SATURN_APP_URL": "https://app.internal.saturnenterprise.io/"
  },
  "git_repositories": [
    {
      "url": "git@github.com:saturncloud/autoshutoff.git",
      "location": "autoshutoff",
      "on_restart": "reclone"
    }
  ],
  "job": {
    "instance_type": "medium",
    "command": "python autoshutoff.py",
    "concurrency_policy": "Forbid",
    "backoff_limit": 1
  }
}