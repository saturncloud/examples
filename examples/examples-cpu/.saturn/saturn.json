{
    "image": "saturncloud/saturn:2020.08.28",
    "jupyter": {
        "size": "large",
        "disk_space": "10Gi",
        "ssh_enabled": false
    },
    "dask_cluster": {
        "n_workers": 3,
        "scheduler_size": "medium",
        "worker_size": "large",
        "nthreads": 2
    },
    "environment_variables": {
        "TAXI_S3": "s3://saturn-public-data/nyc-taxi",
        "MODEL_FILE": "xgboost.pkl",
        "PREFECT_CLOUD_PROJECT_NAME": "dask-iz-gr8"
    },
    "description": "Examples for using Saturn Cloud. Includes Dask and Prefect tutorials, an end-to-end data science pipeline, and more."
}
