{
  "name": "snowflake-ml",
  "image": "saturncloud/saturn-pytorch:2021.09.20",
  "description": "Use Snowflake unstructured data for image classification with Dask",
  "environment_variables": {
    "DASK_DISTRIBUTED__WORKER__DAEMON": "False",
    "MODEL_S3": "s3://saturn-public-files/examples-data/snowflake-ml/modeltrained.pt",
    "SATURN__JUPYTER_SETUP_DASK_WORKSPACE": "true"
  },
  "working_dir": "/home/jovyan/git-repos/examples/examples/snowflake-ml",
  "start_script": "",
  "git_repos": [
    {
      "remote_url": "https://github.com/saturncloud/examples",
      "ref": "release-2021.09.20",
      "location": "examples"
    }
  ],
  "jupyter_server": {
    "start_ssh": false,
    "disk_space": "10Gi",
    "instance_type": "g4dnxlarge",
    "auto_shutoff": "1 hour"
  },
  "dask_cluster": {
    "n_workers": 3,
    "worker": {
      "instance_type": "large",
      "nprocs": 1,
      "nthreads": 1,
      "is_spot": false
    },
    "scheduler": {
      "instance_type": "large"
    }
  }
}
