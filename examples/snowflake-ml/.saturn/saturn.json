{
  "name": "snowflake-ml",
  "image_uri": "saturncloud/saturn-pytorch:2021.09.20",
  "description": "Use Snowflake unstructured data for image classification with Dask",
  "environment_variables": {
    "DASK_DISTRIBUTED__WORKER__DAEMON": "False",
    "MODEL_S3": "s3://saturn-public-files/examples-data/snowflake-ml/modeltrained.pt",
    "SATURN__JUPYTER_SETUP_DASK_WORKSPACE": "true"
  },
  "working_directory": "/home/jovyan/git-repos/examples/examples/snowflake-ml",
  "git_repositories": [
    {
      "url": "https://github.com/saturncloud/examples",
      "reference": "main",
      "location": "examples"
    }
  ],
  "jupyter_server": {
    "start_ssh": false,
    "disk_space": "10Gi",
    "instance_type": "g4dnxlarge",
    "auto_shutoff": "1 hour"
  },
  "dask_cluster": {
    "num_workers": 3,
    "worker": {
      "instance_type": "large",
      "num_processes": 1,
      "num_threads": 1,
      "use_spot_instances": false
    },
    "scheduler": {
      "instance_type": "large"
    }
  }
}
