{
  "name": "example-api-flask",
  "image_uri": "public.ecr.aws/saturncloud/saturn-python:2023.09.01",
  "description": "Deploy a flask API on Saturn Cloud",
  "extra_packages": {
    "conda": "flask",
    "use_mamba": true
